library(tidyverse)
data.frame(a=c(1,2,3,4,5), b=c('a', 'b', 'a', 'b', 'c')) %>% dplyr::group_by(b) %>% summarize(avg(a))
data.frame(a=c(1,2,3,4,5), b=c('a', 'b', 'a', 'b', 'c')) %>% dplyr::group_by(b) %>% summarize(average(a))
data.frame(a=c(1,2,3,4,5), b=c('a', 'b', 'a', 'b', 'c')) %>% dplyr::group_by(b) %>% summarize(mean(a))
